#9
s1 = str(input("Enter, string : "))
s2 = int(input("Enter, the number you want to duplicate the string : "))

print( s1 * int(s2) )

#10
s1 = str(input("Enter, string_1 : "))
s2 = str(input("Enter, string_2 : "))

if s1 == s2:
    print ("두 문자열이 같다")
else:
    print (s1 + s2)
