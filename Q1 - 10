#4
x = int(input("Enter, number_1: "))
y = int(input("Enter, number_2: "))

if x >y:
    print (x)
else:
    print (y)

#5
AAs = {"ATG": "M"}
codon = str(input("Enter, a nucleic acid sequence : "))
AA = AAs.get(codon)
print(AA)

#6
while True:
    x = int(input("Enter, number_1: "))
    y = int(input("Enter, number_2: "))
    if x >y:
        print (x)
    else:
        print (y)
        check = str(input("Do you want to quit this loop? (Y/N)")).lower().strip()
        if check[0] == "n":
            pass
        if check[0] == "y":
            break

#7
y = int(input("Hello, welcome to multiplication test, select the level : "))

print( "multiplication, X",y )

for i in range(2, 9):
        print( y, 'X', i , '=', y*i )

#8
